    "@testing-library/user-event": "^12.0.14",
    "bitstring": "^1.0.0",
    "buffer": "^5.6.0",
    "bulma": "^0.9.0",
    "chart.js": "^2.9.3",
    "chartjs-plugin-zoom": "^0.7.7",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.7",
    "firebase": "^7.19.0",
    "gzipper": "^4.0.1",
    "js-quantities": "^1.7.5",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "python-struct": "^1.1.3",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.1.0",
    "react-google-button": "^0.7.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-select": "^3.1.0",
    "react-table": "^7.5.0",
    "redux-zero": "^5.1.7",
    "s-ago": "^2.2.0",
    "use-query-params": "^1.1.6"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "coverage": "npm run test -- --coverage --watchAll=false",
    "compress": "npm run compress-gz && npm run compress-br",
    "compress-br": "npx gzipper c --brotli --exclude png,ico,webp,gz,br ./build",
    "compress-gz": "npx gzipper c --level 9 --exclude png,ico,webp,gz,br ./build",
    "start": "react-scripts start",
    "build": "react-scripts build && npm run compress",
    "build-ci": "CI=false react-scripts build",
    "test": "react-scripts test --runInBand",
    "start-prod": "node src/dev/express.js",
    "version": "node bump.js --v && git add src/",
    "postversion": "git push && git push --tags"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "imagemin": "^7.0.1",
    "imagemin-webp": "^6.0.0",
    "jest-each": "^26.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "prompts": "^2.3.2",
    "source-map-explorer": "^2.4.2",
    "yargs": "^15.4.1"
  },
  "engines": {
    "node": "14.x"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/!(.test).js"
    ]
  }
}
